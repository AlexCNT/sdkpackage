
<component
	id="ApsTester"
	title="Aps Tester"
	tooltip="Application Property Sheet Tester"
	icon="res/ApsTester/ApsTester.png"
	handlerType="div"
	propertySheetPath="aps/org.scn.community.PropertyPage.div.html"
	group="ScnCommunityPrototypes"
	databound="true"
>
	
	<jsInclude>res/require_loader.js</jsInclude>
	<jsInclude>res/ApsTester/ApsTesterLoader.js</jsInclude>
	<cssInclude>res/ApsTester/ApsTester.css</cssInclude>
	
	<property
		id="checkboxExample"
		type="boolean"
		title="Ignore Totals"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="checkboxOnlyGetterExample"
		type="boolean"
		title="Ignore Totals"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="colorExample"
		type="Color"
		title="Simple Color"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="comboboxExample"
		type="String"
		title="Simple Choice"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="data"
		type="ResultSet"
		title="Data"
		group="Data"
		visible="true">

		<option name="includeFormattedData" value="true"/>
		<option name="includeData" value="true"/>
	</property>
	<property
		id="doublearray"
		type="String"
		title="Complex Array"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="paletteExample"
		type="String"
		title="Color Palette"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="singlearray"
		type="String"
		title="Single Array"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="spinnerExample"
		type="int"
		title="Some numbers"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="stringarray"
		type="String"
		title="Simple String Array"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="textExample"
		type="String"
		title="A saple text"
		group="Display"
		bindable="true"
		visible="true">

	</property>


	<initialization>
		<defaultValue property="TOP_MARGIN">0</defaultValue>
		<defaultValue property="BOTTOM_MARGIN">auto</defaultValue>
		<defaultValue property="LEFT_MARGIN">0</defaultValue>
		<defaultValue property="RIGHT_MARGIN">auto</defaultValue>
		<defaultValue property="WIDTH">200</defaultValue>
		<defaultValue property="HEIGHT">200</defaultValue>
		
		<defaultValue property="checkboxExample">true</defaultValue>
		<defaultValue property="checkboxOnlyGetterExample">true</defaultValue>
		<defaultValue property="colorExample"></defaultValue>
		<defaultValue property="comboboxExample">quantile</defaultValue>
		<defaultValue property="data">null</defaultValue>
		<defaultValue property="doublearray">[]</defaultValue>
		<defaultValue property="paletteExample"></defaultValue>
		<defaultValue property="singlearray">[]</defaultValue>
		<defaultValue property="spinnerExample"></defaultValue>
		<defaultValue property="stringarray">[]</defaultValue>
		<defaultValue property="textExample">initial</defaultValue>

	</initialization>

</component>
