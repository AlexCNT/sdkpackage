
<component
	id="ApsTesterUI5"
	title="Aps Tester UI5"
	tooltip="Application Property Sheet Tester UI5"
	icon="res/ApsTesterUI5/ApsTesterUI5.png"
	handlerType="sapui5"
	propertySheetPath="res/ApsTesterUI5/aps/PropertyPage.html"
	group="ScnCommunityPrototypes"
	databound="true"
>
	
	<jsInclude>res/require_loader.js</jsInclude>
	<jsInclude>res/ApsTesterUI5/ApsTesterUI5Loader.js</jsInclude>
	<cssInclude>res/ApsTesterUI5/ApsTesterUI5.css</cssInclude>
	
	<property
		id="comboboxExample"
		type="String"
		title="Simple Choice"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="colorExample"
		type="String"
		title="Simple Color"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="data"
		type="ResultSet"
		title="Data"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="checkboxExample"
		type="boolean"
		title="Ignore Totals"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="checkboxOnlyGetterExample"
		type="boolean"
		title="Ignore Totals"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="paletteExample"
		type="String"
		title="Color Palette"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="textExample"
		type="String"
		title="A saple text"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="spinnerExample"
		type="int"
		title="Some numbers"
		group="Display"
		bindable="true"
		visible="true">

	</property>


	<initialization>
		<defaultValue property="TOP_MARGIN">0</defaultValue>
		<defaultValue property="BOTTOM_MARGIN">auto</defaultValue>
		<defaultValue property="LEFT_MARGIN">0</defaultValue>
		<defaultValue property="RIGHT_MARGIN">auto</defaultValue>
		<defaultValue property="WIDTH">200</defaultValue>
		<defaultValue property="HEIGHT">200</defaultValue>
		
		<defaultValue property="comboboxExample">quantile</defaultValue>
		<defaultValue property="colorExample"></defaultValue>
		<defaultValue property="data">null</defaultValue>
		<defaultValue property="checkboxExample">true</defaultValue>
		<defaultValue property="checkboxOnlyGetterExample">true</defaultValue>
		<defaultValue property="paletteExample"></defaultValue>
		<defaultValue property="textExample">initial</defaultValue>
		<defaultValue property="spinnerExample"></defaultValue>

	</initialization>

</component>
