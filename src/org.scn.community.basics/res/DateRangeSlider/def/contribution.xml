
<component
	id="DateRangeSlider"
	title="Date Range Slider 2.0"
	tooltip="Date Range Slider"
	icon="res/DateRangeSlider/DateRangeSlider.png"
	handlerType="sapui5"
	propertySheetPath="res/DateRangeSlider/aps/PropertyPage.html"
	group="ScnCommunityBasics"
	databound="false"
>
	
	<jsInclude>res/require_loader.js</jsInclude>
	<jsInclude>res/DateRangeSlider/DateRangeSliderLoader.js</jsInclude>
	<cssInclude>res/DateRangeSlider/DateRangeSlider.css</cssInclude>
	
	<property
		id="DGranulatiry"
		type="String"
		title="Granularity"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="DLangFormat"
		type="boolean"
		title="Language Dependent Format"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="DLangPattern"
		type="String"
		title="Custom Format Pattern"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="DLangRelative"
		type="boolean"
		title="Custom Format Relative"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="DLangStyle"
		type="String"
		title="Custom Format Style"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="DMax"
		type="String"
		title="Maximum Date Value (yyyymmdd)"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="DMin"
		type="String"
		title="Minimum Date Value (yyyymmdd)"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="DValue"
		type="String"
		title="First Date Value (yyyymmdd)"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="DValue2"
		type="String"
		title="Second Date Value (yyyymmdd)"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="onChange"
		type="ScriptText"
		title="On Change"
		group="Events"
		visible="true">

	</property>
	<property
		id="showBubbles"
		type="boolean"
		title="Show Bubbles"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="smallStepWidth"
		type="String"
		title="Small Step Width"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="stepLabels"
		type="boolean"
		title="Step Labels"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="totalUnits"
		type="int"
		title="Total Units"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="vertical"
		type="boolean"
		title="Change To Vertical Orientation"
		group="Display"
		bindable="true"
		visible="true">

	</property>


	<initialization>
		<defaultValue property="TOP_MARGIN">0</defaultValue>
		<defaultValue property="BOTTOM_MARGIN">auto</defaultValue>
		<defaultValue property="LEFT_MARGIN">0</defaultValue>
		<defaultValue property="RIGHT_MARGIN">auto</defaultValue>
		<defaultValue property="WIDTH">300</defaultValue>
		<defaultValue property="HEIGHT">30</defaultValue>
		
		<defaultValue property="DGranulatiry">Day</defaultValue>
		<defaultValue property="DLangFormat">false</defaultValue>
		<defaultValue property="DLangPattern"></defaultValue>
		<defaultValue property="DLangRelative">false</defaultValue>
		<defaultValue property="DLangStyle">Short</defaultValue>
		<defaultValue property="DMax"></defaultValue>
		<defaultValue property="DMin"></defaultValue>
		<defaultValue property="DValue"></defaultValue>
		<defaultValue property="DValue2"></defaultValue>
		<defaultValue property="onChange"></defaultValue>
		<defaultValue property="showBubbles">true</defaultValue>
		<defaultValue property="smallStepWidth">1</defaultValue>
		<defaultValue property="stepLabels"></defaultValue>
		<defaultValue property="totalUnits">0</defaultValue>
		<defaultValue property="vertical">false</defaultValue>

	</initialization>

</component>
