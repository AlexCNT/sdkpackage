
<component
	id="KpiTile"
	title="Kpi Tile 2.0"
	tooltip="Kpi Tile - Custom KPI Visualization"
	icon="res/KpiTile/KpiTile.png"
	handlerType="sapui5"
	propertySheetPath="res/KpiTile/aps/PropertyPage.html"
	group="ScnCommunityBasics"
	databound="false"
>
	
	<jsInclude>res/require_loader.js</jsInclude>
	<jsInclude>res/KpiTile/KpiTileLoader.js</jsInclude>
	<cssInclude>res/KpiTile/KpiTile.css</cssInclude>
	
	<property
		id="componentsSpec"
		type="String"
		title="Components"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="onClick"
		type="ScriptText"
		title="On Click"
		group="Events"
		visible="true">

	</property>


	<initialization>
		<defaultValue property="TOP_MARGIN">0</defaultValue>
		<defaultValue property="BOTTOM_MARGIN">auto</defaultValue>
		<defaultValue property="LEFT_MARGIN">0</defaultValue>
		<defaultValue property="RIGHT_MARGIN">auto</defaultValue>
		<defaultValue property="WIDTH">220</defaultValue>
		<defaultValue property="HEIGHT">190</defaultValue>
		
		<defaultValue property="componentsSpec">[]</defaultValue>
		<defaultValue property="onClick"></defaultValue>

	</initialization>

</component>
