
<component
	id="ActivityViewer"
	title="Activity Viewer 2.0"
	tooltip="Viewer for Custom Activities"
	icon="res/ActivityViewer/ActivityViewer.png"
	handlerType="div"
	propertySheetPath="aps/org.scn.community.PropertyPage.div.html"
	group="ScnCommunityBasics"
	databound="false"
>
	
	<jsInclude>res/require_loader.js</jsInclude>
	<jsInclude>res/ActivityViewer/ActivityViewerLoader.js</jsInclude>
	<cssInclude>res/ActivityViewer/ActivityViewer.css</cssInclude>
	
	<property
		id="activities"
		type="String"
		title="Activities"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="categories"
		type="String"
		title="Categories"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="onSelectionChanged"
		type="ScriptText"
		title="On Selection Changed"
		group="Events"
		visible="true">

	</property>
	<property
		id="states"
		type="String"
		title="States"
		group="Display"
		bindable="true"
		visible="true">

	</property>


	<initialization>
		<defaultValue property="TOP_MARGIN">0</defaultValue>
		<defaultValue property="BOTTOM_MARGIN">auto</defaultValue>
		<defaultValue property="LEFT_MARGIN">0</defaultValue>
		<defaultValue property="RIGHT_MARGIN">auto</defaultValue>
		<defaultValue property="WIDTH">600</defaultValue>
		<defaultValue property="HEIGHT">400</defaultValue>
		
		<defaultValue property="activities"><![CDATA[[]]]></defaultValue>
		<defaultValue property="categories"><![CDATA[[]]]></defaultValue>
		<defaultValue property="onSelectionChanged"><![CDATA[]]></defaultValue>
		<defaultValue property="states"><![CDATA[[]]]></defaultValue>

	</initialization>

</component>
