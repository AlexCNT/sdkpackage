
<component
	id="ActivityViewer"
	title="Activity Viewer (Prototype)"
	tooltip="Viewer for Custom Activities"
	icon="res/ActivityViewer/ActivityViewer.png"
	handlerType="div"
	propertySheetPath="aps/org.scn.community.PropertyPage.div.html"
	group="ScnCommunityBasics"
	databound="false"
>

	<jsInclude>res/require_loader.js</jsInclude>
	<jsInclude>res/ActivityViewer/ActivityViewerLoader.js</jsInclude>
	<cssInclude>res/ActivityViewer/ActivityViewer.css</cssInclude>
	
	<property
		id="activities"
		type="String"
		title="Activities"
		group="Display"
		visible="true">

	</property>
	<property
		id="categories"
		type="String"
		title="Categories"
		group="Display"
		visible="true">

	</property>
	<property
		id="states"
		type="String"
		title="States"
		group="Display"
		visible="true">

	</property>


	<initialization>
		<defaultValue property="TOP_MARGIN">0</defaultValue>
		<defaultValue property="BOTTOM_MARGIN">auto</defaultValue>
		<defaultValue property="LEFT_MARGIN">0</defaultValue>
		<defaultValue property="RIGHT_MARGIN">0</defaultValue>
		<defaultValue property="WIDTH">200</defaultValue>
		<defaultValue property="HEIGHT">200</defaultValue>
		
		<defaultValue property="activities">[]</defaultValue>
		<defaultValue property="categories">[]</defaultValue>
		<defaultValue property="states">[]</defaultValue>

	</initialization>

</component>
