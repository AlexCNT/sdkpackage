
<component id="FioriAppHeader" 
	title="Fiori App Header"
	icon="res/FioriAppHeader/FioriAppHeader.png"
	handlerType="sapui5" 
	propertySheetPath="res/FioriAppHeader/aps/FioriAppHeaderPropertyPage.html"
	group="ScnCommunityBasics">
  	
  	<jsInclude>res/common_basics.js</jsInclude>
	<jsInclude>res/FioriAppHeader/FioriAppHeader.js</jsInclude>
  	
  	<cssInclude>res/FioriAppHeader/FioriAppHeader.css</cssInclude>
  	<property id="title" type="String" title="Title"/>
  	<property id="itemConfig" type="String" title="Item Config" visible="false"/>
  	<property id="showNavButton" type="boolean" title="Show Back Button"/>
  	<property type="String" title="Selected Item" id="selectedItem" visible="false"/>
  	<property type="String" title="Selected Header of Item" id="selectedHeader" visible="false"/>
  	<property type="ScriptText" title="On Item Select" id="onitemselect"/>  	
  	<property type="ScriptText" title="On Back" id="onnav"/>
 	<initialization>
 		<defaultValue property="title">Page Title</defaultValue>
 		<defaultValue property="WIDTH">400</defaultValue>
 	</initialization>
 </component>