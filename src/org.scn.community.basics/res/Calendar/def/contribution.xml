
<component
	id="Calendar"
	title="Calendar 2.0"
	tooltip="Calendar"
	icon="res/Calendar/Calendar.png"
	handlerType="sapui5"
	propertySheetPath="res/Calendar/aps/PropertyPage.html"
	group="ScnCommunityBasics"
	databound="true"
>
	
	<jsInclude>res/require_loader.js</jsInclude>
	<jsInclude>res/Calendar/CalendarLoader.js</jsInclude>
	<cssInclude>res/Calendar/Calendar.css</cssInclude>
	
	<property
		id="DCurrentValue"
		type="String"
		title="Current Date Value YYYYMMDD"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="DSelectionType"
		type="String"
		title="Selection Type"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="DValue"
		type="String"
		title="Date Single Value YYYYMMDD"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="DValueF"
		type="String"
		title="Date Value Range From YYYYMMDD"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="DValueT"
		type="String"
		title="Date Value Range To YYYYMMDD"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="firstDayOffset"
		type="int"
		title="First Day Offset (0 == Sunday)"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="monthsPerRow"
		type="int"
		title="Months Per Row"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="monthsToDisplay"
		type="int"
		title="Months To Display"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="onCurrentChanged"
		type="ScriptText"
		title="Event For On Current Value Changed"
		group="Events"
		visible="true">

	</property>
	<property
		id="onRangeChanged"
		type="ScriptText"
		title="Event For On Value Change With Date Range"
		group="Events"
		visible="true">

	</property>
	<property
		id="onSingleChanged"
		type="ScriptText"
		title="Event For On Value Change With Single Date"
		group="Events"
		visible="true">

	</property>
	<property
		id="singleRow"
		type="boolean"
		title="Single Row"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="weeksPerRow"
		type="int"
		title="Weeks Per Row"
		group="Display"
		bindable="true"
		visible="true">

	</property>


	<initialization>
		<defaultValue property="TOP_MARGIN">0</defaultValue>
		<defaultValue property="BOTTOM_MARGIN">auto</defaultValue>
		<defaultValue property="LEFT_MARGIN">0</defaultValue>
		<defaultValue property="RIGHT_MARGIN">auto</defaultValue>
		<defaultValue property="WIDTH">300</defaultValue>
		<defaultValue property="HEIGHT">300</defaultValue>
		
		<defaultValue property="DCurrentValue">TODAY</defaultValue>
		<defaultValue property="DSelectionType">Single</defaultValue>
		<defaultValue property="DValue">TODAY</defaultValue>
		<defaultValue property="DValueF">TODAY</defaultValue>
		<defaultValue property="DValueT">TODAY</defaultValue>
		<defaultValue property="firstDayOffset">1</defaultValue>
		<defaultValue property="monthsPerRow">1</defaultValue>
		<defaultValue property="monthsToDisplay">1</defaultValue>
		<defaultValue property="onCurrentChanged"></defaultValue>
		<defaultValue property="onRangeChanged"></defaultValue>
		<defaultValue property="onSingleChanged"></defaultValue>
		<defaultValue property="singleRow">false</defaultValue>
		<defaultValue property="weeksPerRow">1</defaultValue>

	</initialization>

</component>
