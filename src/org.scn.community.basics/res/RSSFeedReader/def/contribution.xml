
<component
	id="RSSFeedReader"
	title="RSSFeedReader 2.0"
	tooltip="RSSFeedReader"
	icon="res/RSSFeedReader/RSSFeedReader.png"
	handlerType="div"
	propertySheetPath="aps/org.scn.community.PropertyPage.div.html"
	group="ScnCommunityBasics"
	databound="false"
>
	
	<jsInclude>res/require_loader.js</jsInclude>
	<jsInclude>res/RSSFeedReader/RSSFeedReaderLoader.js</jsInclude>
	<cssInclude>res/RSSFeedReader/RSSFeedReader.css</cssInclude>
	
	<property
		id="feedUrl"
		type="Url"
		title="RSS Feed URL"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="useBuildInXsl"
		type="boolean"
		title="Use Build-In Xsl File"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="xslUrl"
		type="Url"
		title="XSL URL"
		group="Display"
		bindable="true"
		visible="true">

	</property>


	<initialization>
		<defaultValue property="TOP_MARGIN">0</defaultValue>
		<defaultValue property="BOTTOM_MARGIN">auto</defaultValue>
		<defaultValue property="LEFT_MARGIN">0</defaultValue>
		<defaultValue property="RIGHT_MARGIN">auto</defaultValue>
		<defaultValue property="WIDTH">300</defaultValue>
		<defaultValue property="HEIGHT">200</defaultValue>
		
		<defaultValue property="feedUrl">http://cdn.eex.com/rss/de/market.rss</defaultValue>
		<defaultValue property="useBuildInXsl">true</defaultValue>
		<defaultValue property="xslUrl"></defaultValue>

	</initialization>

</component>
