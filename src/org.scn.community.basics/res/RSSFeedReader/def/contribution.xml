
<component
	id="RSSFeedReader"
	title="RSSFeedReader"
	icon="res/RSSFeedReader/RSSFeedReader.png"
	handlerType="div"
	group="ScnCommunityBasics">
	
	<jsInclude>res/require_loader.js</jsInclude>
	<jsInclude>res/common_basics.js</jsInclude>
	<jsInclude>res/RSSFeedReader/RSSFeedReader.js</jsInclude>
	<cssInclude>res/RSSFeedReader/RSSFeedReader.css</cssInclude>
	<property group="Display" type="String" title="RSS Feed URL"
		id="feedurldimension">
	</property>
	<property group="Display" type="String" title="XSL URL"
		id="xslurldimension">
	</property>
	<initialization>
		<defaultValue property="TOP_MARGIN">0</defaultValue>
		<defaultValue property="LEFT_MARGIN">0</defaultValue>
		<defaultValue property="RIGHT_MARGIN">auto</defaultValue>
		<defaultValue property="BOTTOM_MARGIN">auto</defaultValue>
		<defaultValue property="WIDTH">300</defaultValue>
		<defaultValue property="HEIGHT">200</defaultValue>
		<defaultValue property="runtimedata">{}</defaultValue>
		<defaultValue property="feedurldimension">
			http://cdn.eex.com/rss/de/market.rss
		</defaultValue>
		<defaultValue property="xslurldimension">
			rss.xsl
		</defaultValue>
	</initialization>

</component>
