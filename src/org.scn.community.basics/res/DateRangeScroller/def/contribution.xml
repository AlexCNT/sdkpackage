
<component
	id="DateRangeScroller"
	title="Date Range Scroller 2.0"
	tooltip="Date Range Scroller"
	icon="res/DateRangeScroller/DateRangeScroller.png"
	handlerType="sapui5"
	propertySheetPath="res/DateRangeScroller/aps/PropertyPage.html"
	group="ScnCommunityBasics"
	databound="false"
>
	
	<jsInclude>res/require_loader.js</jsInclude>
	<jsInclude>res/DateRangeScroller/DateRangeScrollerLoader.js</jsInclude>
	<cssInclude>res/DateRangeScroller/DateRangeScroller.css</cssInclude>
	
	<property
		id="DDate"
		type="String"
		title="Date (yyyymmdd)"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="DEndDate"
		type="String"
		title="Date (yyyymmdd)"
		group="Display"
		bindable="true"
		visible="false">

	</property>
	<property
		id="DLangFormat"
		type="boolean"
		title="Language Dependent Format"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="DLangPattern"
		type="String"
		title="Custom Format Pattern"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="DLangRelative"
		type="boolean"
		title="Custom Format Relative"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="DLangStyle"
		type="String"
		title="Custom Format Style"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="DSpecialDay"
		type="int"
		title="Day Of Week To Start (CustomWeek)"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="DSpecialDuration"
		type="int"
		title="Duration Of Custom Range (Custom*)"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="DStartDate"
		type="String"
		title="Start Date (yyyymmdd)"
		group="Display"
		bindable="true"
		visible="false">

	</property>
	<property
		id="DViewType"
		type="String"
		title="Range View Type"
		group="Display"
		bindable="true"
		visible="true">

	</property>
	<property
		id="editable"
		type="boolean"
		title="Editable"
		group="Display"
		bindable="true"
		visible="false">

	</property>
	<property
		id="onChange"
		type="ScriptText"
		title="On Change"
		group="Events"
		visible="true">

	</property>


	<initialization>
		<defaultValue property="TOP_MARGIN">0</defaultValue>
		<defaultValue property="BOTTOM_MARGIN">auto</defaultValue>
		<defaultValue property="LEFT_MARGIN">0</defaultValue>
		<defaultValue property="RIGHT_MARGIN">auto</defaultValue>
		<defaultValue property="WIDTH">300</defaultValue>
		<defaultValue property="HEIGHT">30</defaultValue>
		
		<defaultValue property="DDate"></defaultValue>
		<defaultValue property="DEndDate"></defaultValue>
		<defaultValue property="DLangFormat">false</defaultValue>
		<defaultValue property="DLangPattern"></defaultValue>
		<defaultValue property="DLangRelative">false</defaultValue>
		<defaultValue property="DLangStyle">Short</defaultValue>
		<defaultValue property="DSpecialDay">0</defaultValue>
		<defaultValue property="DSpecialDuration">14</defaultValue>
		<defaultValue property="DStartDate"></defaultValue>
		<defaultValue property="DViewType">Week</defaultValue>
		<defaultValue property="editable">true</defaultValue>
		<defaultValue property="onChange"></defaultValue>

	</initialization>

</component>
