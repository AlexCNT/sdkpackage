
<component
	id="MicroChart"
	title="Micro Chart 2.0"
	tooltip="Micro Chart - Component for VIsualization of Data on small areas"
	icon="res/MicroChart/MicroChart.png"
	handlerType="sapui5"
	propertySheetPath="res/MicroChart/aps/PropertyPage.html"
	group="ScnCommunityDataBound"
	databound="true"
>
	
	<jsInclude>res/require_loader.js</jsInclude>
	<jsInclude>res/MicroChart/MicroChartLoader.js</jsInclude>
	<cssInclude>res/MicroChart/MicroChart.css</cssInclude>
	
	<property
		id="dataCellListActual"
		type="ResultCellList"
		title="Actual Data List"
		group="Data"
		visible="true"
		bindable="true">

		<option name="includeFormattedData" value="true"/>
		<option name="includeData" value="true"/>
		<option name="includeMetadata" value="true"/>
	</property>
	<property
		id="dataCellListForecast"
		type="ResultCellList"
		title="Forecast Data List"
		group="Data"
		visible="true"
		bindable="true">

		<option name="includeFormattedData" value="true"/>
		<option name="includeData" value="true"/>
		<option name="includeMetadata" value="true"/>
	</property>
	<property
		id="dataCellListTarget"
		type="ResultCellList"
		title="Target Data List"
		group="Data"
		visible="true"
		bindable="true">

		<option name="includeFormattedData" value="true"/>
		<option name="includeData" value="true"/>
		<option name="includeMetadata" value="true"/>
	</property>
	<property
		id="dataCellListThreshold1"
		type="ResultCellList"
		title="Treshold 2 Data List"
		group="Data"
		visible="true"
		bindable="true">

		<option name="includeFormattedData" value="true"/>
		<option name="includeData" value="true"/>
		<option name="includeMetadata" value="true"/>
	</property>
	<property
		id="dataCellListThreshold2"
		type="ResultCellList"
		title="Treshold 2 Data List"
		group="Data"
		visible="true"
		bindable="true">

		<option name="includeFormattedData" value="true"/>
		<option name="includeData" value="true"/>
		<option name="includeMetadata" value="true"/>
	</property>
	<property
		id="dataCellListThreshold3"
		type="ResultCellList"
		title="Treshold 3 Data List"
		group="Data"
		visible="true"
		bindable="true">

		<option name="includeFormattedData" value="true"/>
		<option name="includeData" value="true"/>
		<option name="includeMetadata" value="true"/>
	</property>
	<property
		id="dataCellListThreshold4"
		type="ResultCellList"
		title="Treshold 4 Data List"
		group="Data"
		visible="true"
		bindable="true">

		<option name="includeFormattedData" value="true"/>
		<option name="includeData" value="true"/>
		<option name="includeMetadata" value="true"/>
	</property>
	<property
		id="dataCellListThreshold5"
		type="ResultCellList"
		title="Treshold 5 Data List"
		group="Data"
		visible="true"
		bindable="true">

		<option name="includeFormattedData" value="true"/>
		<option name="includeData" value="true"/>
		<option name="includeMetadata" value="true"/>
	</property>


	<initialization>
		<defaultValue property="TOP_MARGIN">0</defaultValue>
		<defaultValue property="BOTTOM_MARGIN">auto</defaultValue>
		<defaultValue property="LEFT_MARGIN">0</defaultValue>
		<defaultValue property="RIGHT_MARGIN">auto</defaultValue>
		<defaultValue property="WIDTH">400</defaultValue>
		<defaultValue property="HEIGHT">200</defaultValue>
		
		<defaultValue property="dataCellListActual"><![CDATA[null]]></defaultValue>
		<defaultValue property="dataCellListForecast"><![CDATA[null]]></defaultValue>
		<defaultValue property="dataCellListTarget"><![CDATA[null]]></defaultValue>
		<defaultValue property="dataCellListThreshold1"><![CDATA[null]]></defaultValue>
		<defaultValue property="dataCellListThreshold2"><![CDATA[null]]></defaultValue>
